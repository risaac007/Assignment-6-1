package interfaces;

/**
 *
 * @author hp1
 */
public class Interfaces {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        // TODO code application logic here
        Class1 c1=new Class1();
        Class2 c2=new Class2();
        c1.input(5);
        c2.input(10);
        
        
        
        
    }
    
}
